{
  "name": "react-advanced-boilerplate",
  "version": "0.2.0",
  "description": "An advanced React boilerplate with PostCSS, Eslint, StyleLint, a release manager, prettier, Storybook and internazionalization out of the box",
  "homepage": "https://github.com/CloudPower97/react-advanced-boilerplate",
  "repository": {
    "type": "git",
    "url": "https://github.com/CloudPower97/react-advanced-boilerplate.git"
  },
  "keywords": [
    "react",
    "boilerplate",
    "scaffolding",
    "i18n"
  ],
  "author": {
    "name": "Claudio Cortese",
    "url": "https://github.com/CloudPower97"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/CloudPower97/react-advanced-boilerplate/issues"
  },
  "dependencies": {
    "i18next": "^13.0.0",
    "i18next-browser-languagedetector": "^2.2.4",
    "i18next-chained-backend": "^0.1.2",
    "i18next-localstorage-backend": "^1.1.4",
    "i18next-xhr-backend": "^1.5.1",
    "react": "^16.6.3",
    "react-dom": "^16.6.3",
    "react-helmet": "^5.2.0",
    "react-i18next": "^8.3.9",
    "react-router-dom": "^4.3.1",
    "react-router-hash-link": "^1.2.1",
    "react-scripts": "2.1.1"
  },
  "scripts": {
    "cypress:run": "cypress run",
    "cypress:open": "cypress open",
    "start": "craco start",
    "test": "npm-run-all test:unit",
    "test:unit": "craco test --env=jsdom",
    "test:e2e": "npm-run-all test:e2e:run",
    "test:e2e:open": "cross-env BROWSER=none server-test start http-get://localhost:3000 cypress:open",
    "test:e2e:run": "cross-env BROWSER=none server-test start http-get://localhost:3000 cypress:run",
    "test:coverage": "craco test --env=jsdom --coverage",
    "test:all": "cross-env CI=true run-s test:unit test:e2e",
    "build": "npm-run-all build:production",
    "build:staging": "env-cmd .env.staging craco build",
    "build:production": "env-cmd .env.production craco build",
    "lint:js": "eslint src/**/**/*.js",
    "lint:js:fix": "eslint src/**/*.js --fix",
    "lint:css": "stylelint src/**/**/*.css",
    "lint:css:fix": "stylelint src/**/**/*.css --fix",
    "pretty:js": "prettier --write src/**/**/*.js",
    "pretty:css": "prettier --write src/**/*.css",
    "storybook": "start-storybook -p 9009 -s public",
    "build-storybook": "build-storybook -s public"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "lint-staged": {
    "src/**/*.js": [
      "yarn pretty:js",
      "yarn lint:js:fix",
      "git add"
    ],
    "src/**/*.css": [
      "yarn pretty:css",
      "yarn lint:css:fix",
      "git add"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.2.2",
    "@babel/plugin-transform-react-constant-elements": "^7.2.0",
    "@babel/plugin-transform-react-inline-elements": "^7.2.0",
    "@commitlint/cli": "^7.2.1",
    "@commitlint/config-angular": "^7.1.2",
    "@craco/craco": "^3.2.3",
    "@storybook/addon-a11y": "^4.1.1",
    "@storybook/addon-actions": "^4.1.1",
    "@storybook/addon-info": "^4.1.1",
    "@storybook/addon-knobs": "^4.1.1",
    "@storybook/addon-links": "^4.1.1",
    "@storybook/addon-options": "^4.1.1",
    "@storybook/addon-viewport": "^4.1.1",
    "@storybook/addons": "^4.1.1",
    "@storybook/react": "^4.1.1",
    "babel-loader": "^8.0.4",
    "babel-plugin-module-resolver": "^3.1.1",
    "babel-plugin-transform-imports": "^1.5.1",
    "babel-plugin-transform-react-class-to-function": "^1.2.0",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.21",
    "conventional-changelog-cli": "^2.0.11",
    "cross-env": "^5.2.0",
    "cssnano": "^4.1.7",
    "cypress": "^3.1.3",
    "directory-named-webpack-plugin": "^4.0.0",
    "env-cmd": "^8.0.2",
    "enzyme": "^3.8.0",
    "enzyme-adapter-react-16": "^1.7.1",
    "enzyme-to-json": "^3.3.5",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-plugin-cypress": "^2.1.3",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jest": "^22.1.2",
    "eslint-plugin-jsx-a11y": "^6.1.2",
    "eslint-plugin-prettier": "^3.0.0",
    "husky": "^1.2.1",
    "imagemin-giflossy": "^5.1.10",
    "imagemin-jpeg-recompress": "^6.0.0",
    "imagemin-mozjpeg": "^8.0.0",
    "imagemin-pngquant": "^6.0.0",
    "imagemin-webp": "^5.0.0",
    "imagemin-webpack-plugin": "^2.3.0",
    "imagemin-zopfli": "^5.1.0",
    "lint-staged": "^8.0.4",
    "npm-run-all": "^4.1.5",
    "postcss-assets": "^5.0.0",
    "postcss-brand-colors": "^3.0.0",
    "postcss-class-name-shortener": "^1.1.0",
    "postcss-color-function": "^4.0.1",
    "postcss-easing-gradients": "^3.0.1",
    "postcss-font-magician": "^2.2.1",
    "postcss-modules-values-replace": "^3.0.1",
    "postcss-preset-env": "^6.5.0",
    "postcss-pseudo-content-insert": "^0.3.0",
    "postcss-will-change-transition": "^1.2.0",
    "prettier": "^1.15.3",
    "react-test-renderer": "^16.6.3",
    "release-it": "^9.1.0",
    "start-server-and-test": "^1.7.11",
    "storybook-react-router": "^1.0.2",
    "stylelint": "^9.9.0",
    "stylelint-a11y": "^1.1.9",
    "stylelint-config-prettier": "^4.0.0",
    "stylelint-csstree-validator": "^1.3.0",
    "stylelint-declaration-block-no-ignored-properties": "^1.1.0",
    "stylelint-high-performance-animation": "^1.1.1",
    "stylelint-images": "^2.0.0",
    "stylelint-no-indistinguishable-colors": "^1.2.1",
    "stylelint-order": "^2.0.0",
    "stylelint-selector-no-empty": "^1.0.7",
    "webpack-bundle-analyzer": "^3.0.3"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
